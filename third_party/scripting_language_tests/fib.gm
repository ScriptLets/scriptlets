global fibR = function(n)
{
    if (n < 2) {return n;}
    return (fibR(n-2) + fibR(n-1));
};


global fibI = function(n)
{
    local last = 0;
    local cur = 1;
    n = n - 1;
    while(n)
    {
        n -= 1;
        local tmp = cur;
        cur = last + cur;
        last = tmp;
    }
    return cur;
};

N = 43;
print("fib: " + fibR(N) + " = " + fibI(N));
